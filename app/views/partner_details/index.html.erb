<p id="notice"><%= notice %></p>

<h1>Partner Details</h1>

<table>
  <thead>
    <tr>
      <th>Partner name</th>
      <th>Amount</th>
      <th>Credit</th>
      <th>Debit</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partner_details.each do |partner_detail| %>
      <tr>
        <td><%= partner_detail.partner_name %></td>
        <td><%= partner_detail.amount %></td>
        <td><%= partner_detail.credit %></td>
        <td><%= partner_detail.debit %></td>
        <td><%= partner_detail.description %></td>
        <td><%= link_to 'Show', partner_detail %></td>
        <td><%= link_to 'Edit', edit_partner_detail_path(partner_detail) %></td>
        <td><%= link_to 'Destroy', partner_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partner Detail', new_partner_detail_path %>
